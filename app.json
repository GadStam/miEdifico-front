{
  "expo": {
    "name": "myapp",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoMI.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "custom photos permission"
        }
      ]
    ],
    "splash": {
      "image": "./assets/logoMI.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "miEdificio.com"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoMI.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "miEdificio.com",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/logoMI.png"
    },
    "extra": {
      "eas": {
        "projectId": "50556deb-ab7b-4850-b82e-4091add5f37e"
      }
    }
  }
}
